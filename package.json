{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "A simple SSR boilerplate using Preact",
  "main": "server.js",
  "scripts": {
    "build": "webpack",
    "watch:webpack": "webpack --watch",
    "watch:reload": "NODE_ENV=test nodemon server.js",
    "watch:windows": "set NODE_ENV=test&&nodemon server.js",
    "dev": "npm-run-all --parallel watch:webpack watch:reload",
    "dev:windows": "npm-run-all --parallel watch:webpack watch:windows",
    "serve": "NODE_ENV=production node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DavidLi-Minxiao/react-boilerplate.git"
  },
  "keywords": [
    "react",
    "universal",
    "node",
    "express"
  ],
  "author": "DavidLi-Minxiao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DavidLi-Minxiao/react-boilerplate/issues"
  },
  "homepage": "https://github.com/DavidLi-Minxiao/davidli-3#readme",
  "dependencies": {
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.23.0",
    "compression": "^1.6.2",
    "express": "^4.14.1",
    "lodash": "^4.17.4",
    "preact": "^7.2.0",
    "preact-compat": "~3.13.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-onclickoutside": "^5.9.0",
    "react-router-dom": "^4.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "eslint": "^3.16.1",
    "eslint-config-standard": "^7.0.0",
    "eslint-config-standard-jsx": "^3.3.0",
    "eslint-config-standard-react": "^4.2.0",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.10.0",
    "eslint-plugin-standard": "^2.0.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  }
}
